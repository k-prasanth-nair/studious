package com.ds;

public class Stack<T> {
	
	private Node<T> top;
	
	public void push(T data){
		Node<T> temp = new Node(data,null);
		temp.next  = top;
		top = temp;
	}
	
	
	private static class Node<T>{
		T data;
		Node<T> next ;
		
		public Node(T data,Node<T> n)
		{
		    this.data = data;
		    this.next = n;
		}
	}

}
