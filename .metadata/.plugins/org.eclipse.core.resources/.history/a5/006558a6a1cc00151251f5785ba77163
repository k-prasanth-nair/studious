package com.ds;



public class Queue<T> {
	
	private Node<T> head;
	private Node<T> last;
	
	public void enQueue(T data){
		Node<T> newNode =  new Node(data,null);
		if(head == null){
			head = newNode;
			last = newNode;				
		}else{			
			last.next = newNode;
			last = newNode;
		}
	
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	private static class Node<T>{
		T data;
		Node<T> next ;
		
		public Node(T data,Node<T> n)
		{
		    this.data = data;
		    this.next = n;
		}
	}

}
