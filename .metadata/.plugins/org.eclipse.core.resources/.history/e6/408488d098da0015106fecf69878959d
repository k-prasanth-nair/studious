package com.ds.arrays;

public class QuickSort {
	
	
	
	
	
	public static void sort(int[] arr){
		sort( arr, 0, arr.length - 1 );
	}
	
	
	private static void sort(int[] arr,int low, int high){
		if( high<= low ) return;
		int j = partition( a, low, high);
		sort(arr,low,j-1);
		sort(arr,j+1,high);
	}
	
	
